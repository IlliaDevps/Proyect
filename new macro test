fetch(url, {
  method: 'GET',
  headers: {
    'Accept': 'application/json',
    'Authorization': `Basic ${btoa('i.duverkher:4i8c66j5q2o344ercc7kmlrq0u9mofq')}`
  },
  rejectUnauthorized: false
})
  .then(response => {
    console.log(`Response: ${response.status} ${response.statusText}`);
    return response.json();
  })
  .then(data => {
    console.log(data);
  })
  .catch(error => {
    console.error(error);
  });
